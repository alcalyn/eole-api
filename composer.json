{
    "name":         "eole/eole-api",
    "license":      "MIT",
    "type":         "library",
    "description":  "Eole",
    "homepage":     "https://github.com/alcalyn/eole-api",
    "keywords":     ["silex", "user", "userprovider"],
    "authors": [
        {
            "name": "Julien Maulny",
            "email": "doubjulien@hotmail.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5",
        "cube/composer-dist-installer": "~1.0@beta",
        "alcalyn/elo": "~1.0.0",
        "doctrine/dbal": "~2.0",
        "doctrine/orm": "~2.0",
        "symfony/yaml": "~2.0",
        "symfony/event-dispatcher": "~3.0",
        "symfony/security": "~2.0",
        "symfony/http-kernel": "~2.0",
        "symfony/http-foundation": "~2.0",
        "symfony/routing": "~2.7|~3.0",
        "symfony/console": "~2.0",
        "silex/silex": "~2.0@dev",
        "jms/serializer": "~1.0",
        "alcalyn/symfony-wsse": "1.0.x",
        "alcalyn/silex-wsse": "1.0.x",
        "alcalyn/user-api": "^1.1.1",
        "alcalyn/doctrine-user-api": "1.0.x",
        "saxulum/saxulum-doctrine-orm-manager-registry-provider": "~3.0",
        "react/zmq": "0.2.*|0.3.*",
        "cboden/ratchet": "~0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "symfony/browser-kit": "~2.0",
        "symfony/process": "~2.0",
        "symfony/css-selector": "~2.0",
        "ekho/zmq-phpdoc": "1.0",
        "squizlabs/php_codesniffer": "2.x"
    },
    "autoload": {
        "psr-4": {
            "Eole\\": "src/Eole",
            "Alcalyn\\Wsse\\": "ext/symfony-wsse",
            "Alcalyn\\SilexWsse\\": "ext/silex-wsse",
            "Alcalyn\\UserApi\\": "ext/user-api",
            "Alcalyn\\DoctrineUserApi\\": "ext/doctrine-user-api",
            "Alcalyn\\": "src/Alcalyn"
        }
    },
    "scripts": {
        "dump-dist": [
            "Cube\\ComposerDistInstaller\\Bootstrap::install"
        ]
    },
    "extra": {
        "dist-installer-params": [
            {
                "file": "config/environment.yml"
            },
            {
                "file": "config/environment_test.yml"
            }
        ]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ekho/zmq-phpdoc",
                "version": "1.0",
                "dist": {
                    "url": "https://raw.githubusercontent.com/ekho/zmq-phpdoc/master/zmq.php",
                    "type": "file"
                }
            }
        }
    ]
}
