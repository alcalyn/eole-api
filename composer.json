{
    "name":         "eole/eole-api",
    "license":      "GPL-3.0",
    "type":         "library",
    "description":  "Eole API.",
    "homepage":     "https://github.com/alcalyn/eole-api",
    "keywords":     ["silex", "eole", "api"],
    "authors": [
        {
            "name": "Julien Maulny",
            "email": "doubjulien@hotmail.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5",
        "alcalyn/authorization-header-fix": "~1.0.0",
        "alcalyn/serializer-doctrine-proxies": "~1.0.0",
        "alcalyn/elo": "~1.0.0",
        "jdesrosiers/silex-cors-provider": "dev-master",
        "league/oauth2-server": "4.1.x",
        "doctrine/common": "~2.5",
        "doctrine/dbal": "~2.5",
        "doctrine/orm": "~2.5",
        "symfony/yaml": "~2.8|~3.0",
        "symfony/event-dispatcher": "~3.0",
        "symfony/security": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/http-foundation": "~2.8|~3.0",
        "symfony/routing": "~2.8|~3.0",
        "symfony/console": "~2.8|~3.0",
        "silex/silex": "~2.0@dev",
        "jms-serializer/serializer": "^1.3.3",
        "alcalyn/user-api": "^1.1.1",
        "alcalyn/doctrine-user-api": "1.0.x",
        "saxulum/saxulum-doctrine-orm-manager-registry-provider": "~3.0",
        "react/zmq": "0.2.*|0.3.*",
        "cboden/ratchet": "~0.3",
        "alcalyn/awale": "1.0.x",
        "alcalyn/tictactoe": "1.0.x"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "symfony/browser-kit": "~2.8|~3.0",
        "symfony/process": "~2.8|~3.0",
        "symfony/css-selector": "~2.8|~3.0",
        "symfony/monolog-bridge": "~2.8|~3.0",
        "silex/web-profiler": "~2.0@dev",
        "sorien/silex-dbal-profiler": "~2.0@dev",
        "ekho/zmq-phpdoc": "1.0",
        "squizlabs/php_codesniffer": "2.x"
    },
    "autoload": {
        "psr-4": {
            "Alcalyn\\Monkeys\\": "../../monkeys-api/",
            "": "src"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ekho/zmq-phpdoc",
                "version": "1.0",
                "dist": {
                    "url": "https://raw.githubusercontent.com/ekho/zmq-phpdoc/master/zmq.php",
                    "type": "file"
                }
            }
        }
    ]
}
